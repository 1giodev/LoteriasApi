{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "ConnectionStrings": {
    "LoteriasConnection": "${{ secrets.LoteriasConnection }}"
  },
  "AllowedHosts": "*",
  "Apis": {
    "UrlBasePortalDeLoteriasCaixa": "${{ secrets.UrlBasePortalDeLoteriasCaixa }}",
    "UrlBaseIbge": "${{ secrets.UrlBaseIbge }}"
  },
  "Jwt": {
    "Key": "${{ secrets.KeyJwt }}",
    "Issuer": "${{ secrets.Issuer }}",
    "Audience": "${{ secrets.Audience }}"
  }
}
